#include <stdio.h>

int main()
{
    int a[6] = {8,3,6,10,45,12};  // int a0,a1,a2,a3,a4,a5
    int b[6] = {0};
    //char s[] = " this is a book ";
    int i,n;
    
    n = sizeof(a)/sizeof(int);
    for(i=0;i < n;i++){
        printf("a[%d] = %d\n",i,a[i]);
    }
    
    for("a[%d] = %d\n",i,a[i])
        scanf("%d",&b[i]);
        
    for("a[%d] = %d\n",i,a[i])
        printf  b[i]    
    return 0;
}
